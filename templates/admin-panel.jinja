    <div class="app-content content">
        <div class="content-overlay"></div>
        <div class="header-navbar-shadow"></div>
        <div class="content-wrapper">
            <div class="content-body">
                <!-- Data list view starts -->
                <section id="data-list-view" class="data-list-view-header">
                    <div class="action-btns d-none">
                        <div class="btn-dropdown mr-1 mb-1">
                            <div class="btn-group dropdown actions-dropodown">
                            </div>
                        </div>
                    </div>

                    <!-- DataTable starts -->
                    <div class="table-responsive">
                        <table class="table data-list-view">
                            <thead>
                                <tr>
                                    <th>TIMESTAMP</th>
                                    <th>ORDER NUMBER</th>
                                    <th>NAME</th>
                                    <th>PHONE</th>
                                    <th>ADDRESS</th>
                                    <th>ORDER STATUS</th>
                                    <th>ORDER</th>
                                    <th>PRICE</th>
                                    <th>ACTION</th>
                                </tr>
                            </thead>
                            <tbody>
                                {% for list in data %}
                                <tr>
                                    <td class="product-time">{{list['time']}}</td>
                                    <td class="product-number">{{list['number']}}</td>
                                    <td class="product-name">{{list['user']['name']}}</td>
                                    <td class="product-category">{{list['user']['phone_number']}}</td>
                                    <td class="product-address">{{list['user']['address']}}</td>
                                    {% if list['status'] == 'Pending' %}
                                    <td>
                                        <div class="chip chip-info">
                                            <div class="chip-body">
                                                <div class="chip-text">Pending</div>
                                            </div>
                                        </div>
                                    </td>
                                    {% elif list['status'] == 'Out' %}
                                    <td>
                                        <div class="chip chip-warning">
                                            <div class="chip-body">
                                                <div class="chip-text">Out</div>
                                            </div>
                                        </div>
                                    </td>
                                     {% else %}
                                    <td>
                                        <div class="chip chip-success">
                                            <div class="chip-body">
                                                <div class="chip-text">Delivered</div>
                                            </div>
                                        </div>
                                    </td>
                                    {% endif %}
                                    <td class="product-price">{{list['items']}}</td>
                                    <td class="product-price">{{list['total']}} EGP</td>
                                    <td class="product-action">
                                        <span class="action-edit"><i class="feather icon-edit"></i></span>
                                    
                                    </td>
                                </tr>
                               {% endfor %}
                            
                            </tbody>
                        </table>
                    </div>
                    <!-- DataTable ends -->

                    <!-- add new sidebar starts -->
                    <div class="add-new-data-sidebar">
                        <div class="overlay-bg"></div>
                        <div class="add-new-data">
                            <div class="div mt-2 px-2 d-flex new-data-title justify-content-between">
                                <div>
                                    <h4 class="text-uppercase">Edit Order Data</h4>
                                </div>
                                <div class="hide-data-sidebar">
                                    <i class="feather icon-x"></i>
                                </div>
                            </div>
                            <div class="data-items pb-3">
                                <div class="data-fields px-2 mt-3">
                                    <div class="row">
                                        <div class="col-sm-12 data-field-col">
                                            <label for="data-name">Name</label>
                                            <input type="text" class="form-control" id="data-name" disabled>
                                        </div>
                                        <div class="col-sm-12 data-field-col">
                                            <label for="data-category"> Phone Number </label>
                                          <input type="tel" class="form-control" id="data-tel" disabled>
                                        </div>
                                        <div class="col-sm-12 data-field-col">
                                            <label for="data-status">Order Status</label>
                                            <select class="form-control" id="data-status">
                                                <option value="Pending">Pending</option>
                                                <option value="Out">Out</option>
                                                <option value="Delivered">Delivered</option>
                                            </select>
                                        </div>
                                        <div class="col-sm-12 data-field-col">
                                            <label for="data-price">Price</label>
                                            <input type="text" class="form-control" id="data-price" disabled>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="add-data-footer d-flex justify-content-around px-3 mt-2">
                                <div class="add-data-btn">
                                    <button class="btn btn-success action-confirm">Confirm</button>
                                </div>
                                <div class="cancel-data-btn">
                                    <button class="btn btn-outline-danger">Cancel</button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- add new sidebar ends -->
                </section>
                <!-- Data list view end -->

            </div>
        </div>
